import java.util.Scanner;
public class Team
{
public static void main(String[]args){
Scanner sc = new Scanner(System.in);
int n = sc.nextInt();
int counter = 0;
for(int i =0; i < n;i++){
int p = sc.nextInt(), v = sc.nextInt(),l = sc.nextInt();
if(p == 1 & v == 1 | p == 1 & l == 1 | l ==1 & v == 1){
counter++;
}
}
System.out.println(counter);
}
}
